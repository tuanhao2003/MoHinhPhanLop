<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.example.DAL">
    <class name="thongTinSd" table="thongtinsd">

        <meta attribute="class-description">
            This class contains the usage information detail.
        </meta>

        
        <id name="matt" type="int" column="MaTT">
            <generator class="identity"/>
        </id>
        <many-to-one name="thanhvien" class="thanhVien" fetch="join">
            <column name="MaTV" not-null="true"/>
        </many-to-one>
        <many-to-one name="thietbi" class="thietBi" fetch="join">
            <column name="MaTB" not-null="false"/>
        </many-to-one>
        <property name="tgvao" column="TGVao" type="timestamp" />
        <property name="tgmuon" column="TGMuon" type="timestamp" />
        <property name="tgtra" column="TGTra" type="timestamp" />

    </class>
</hibernate-mapping>